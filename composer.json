{
  "name": "blackbonjour/mox-file-reader",
  "prefer-stable": true,
  "type": "library",
  "description": "Library for reading Synetic MOX files used in various World Racing and Crash Time games.",
  "keywords": [
    "MOX",
    "Synetic",
    "World Racing",
    "Crash Time"
  ],
  "homepage": "https://erickdyck.de",
  "version": "0.1.0",
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "phpmd/phpmd": "*",
    "phpstan/phpstan": "*"
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-intl-grapheme": "*",
    "symfony/polyfill-intl-normalizer": "*",
    "symfony/polyfill-mbstring": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "autoload": {
    "psr-4": {
      "BlackBonjour\\Mox\\": "src/"
    }
  }
}
